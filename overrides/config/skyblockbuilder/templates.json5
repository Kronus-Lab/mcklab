{
  // The template which will be used for spawn only
  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "direction": "south",
  //     "offset": [ 0, 0 ]
  // }
  "spawn": null,
  
  "spawns": {
    "default": [
      [ 6, 1, 6 ]
    ]
  },
  
  // The list of templates being available. The first entry is the default template.
  "templates": [
    {
      "name": "default",
      "file": "basic_island.nbt",
      "spawns": "default",
      "direction": "south"
    }
  ]
}
